le,.gssb_a table tr,.gssb_a table tr td{padding:0;margin:0;border:0}</style><style type="text/css">.gsc-control-cse{font-family:georgia, arial, serif}.gsc-control-cse .gsc-table-result{font-family:georgia, arial, serif}.gsc-refinementsGradient{background:linear-gradient(to left,rgba(255,255,255,1),rgba(255,255,255,0))}.gsc-control-cse{border-color:#FFFFFF;background-color:#FFFFFF}input.gsc-input,.gsc-input-box,.gsc-input-box-hover,.gsc-input-box-focus{border-color:#D3BCA1}.gsc-search-button-v2,.gsc-search-button-v2:hover,.gsc-search-button-v2:focus{border-color:#300D00;background-color:#461200;background-image:none;filter:none}.gsc-search-button-v2 svg{fill:#FFFFFF}.gsc-tabHeader.gsc-tabhActive,.gsc-refinementHeader.gsc-refinementhActive{color:#461200;border-color:#461200;background-color:#FFFFFF}.gsc-tabHeader.gsc-tabhInactive,.gsc-refinementHeader.gsc-refinementhInactive{color:#950000;border-color:#950000;background-color:#FFFFFF}.gsc-webResult.gsc-result,.gsc-results .gsc-imageResult{border-color:#FFFFFF;background-color:#FFFFFF}.gsc-webResult.gsc-result:hover{border-color:#FFFFFF;background-color:#FFFFFF}.gs-webResult.gs-result a.gs-title:link,.gs-webResult.gs-result a.gs-title:link b,.gs-imageResult a.gs-title:link,.gs-imageResult a.gs-title:link b{color:#950000}.gs-webResult.gs-result a.gs-title:visited,.gs-webResult.gs-result a.gs-title:visited b,.gs-imageResult a.gs-title:visited,.gs-imageResult a.gs-title:visited b{color:#950000}.gs-webResult.gs-result a.gs-title:hover,.gs-webResult.gs-result a.gs-title:hover b,.gs-imageResult a.gs-title:hover,.gs-imageResult a.gs-title:hover b{color:#950000}.gs-webResult.gs-result a.gs-title:active,.gs-webResult.gs-result a.gs-title:active b,.gs-imageResult a.gs-title:active,.gs-imageResult a.gs-title:active b{color:#950000}.gsc-cursor-page{color:#950000}a.gsc-trailing-more-results:link{color:#950000}.gs-webResult:not(.gs-no-results-result):not(.gs-error-result) .gs-snippet,.gs-fileFormatType{color:#333333}.gs-webResult div.gs-visibleUrl{color:#A25B08}.gs-webResult div.gs-visibleUrl-short{color:#A25B08}.gsc-cursor-box{border-color:#FFFFFF}.gsc-results .gsc-cursor-box .gsc-cursor-page{border-color:#950000;background-color:#FFFFFF;color:#950000}.gsc-results .gsc-cursor-box .gsc-cursor-current-page{border-color:#461200;background-color:#FFFFFF;color:#461200}.gsc-webResult.gsc-result.gsc-promotion{border-color:#FEFEDC;background-color:#FFFFCC}.gsc-completion-title{color:#950000}.gsc-completion-snippet{color:#333333}.gs-promotion a.gs-title:link,.gs-promotion a.gs-title:link *,.gs-promotion .gs-snippet a:link{color:#0000CC}.gs-promotion a.gs-title:visited,.gs-promotion a.gs-title:visited *,.gs-promotion .gs-snippet a:visited{color:#0000CC}.gs-promotion a.gs-title:hover,.gs-promotion a.gs-title:hover *,.gs-promotion .gs-snippet a:hover{color:#0000CC}.gs-promotion a.gs-title:active,.gs-promotion a.gs-title:active *,.gs-promotion .gs-snippet a:active{color:#0000CC}.gs-promotion .gs-snippet,.gs-promotion .gs-title .gs-promotion-title-right,.gs-promotion .gs-title .gs-promotion-title-right *{color:#333333}.gs-promotion .gs-visibleUrl,.gs-promotion .gs-visibleUrl-short{color:#A25B08}.gcsc-find-more-on-google{color:#950000}.gcsc-find-more-on-google-magnifier{fill:#950000}</style><script type="text/javascript" async="" src="https://cdn.adpushup.com/prebid/pb.37780.1667225473670.js"/><script type="text/javascript" async="" src="https://cdn.adpushup.com/pbuseridscripts/quantcast.js"/><script type="text/javascript">function triggerRequest(eventName, data) {
	window.adpushup.utils.sendDataToLoggerService(eventName, data);
}
(function () {
	function injectHeadCodeOnPage(src) {
		var scriptEl = document.createElement('script');
		scriptEl.type = 'text/javascript';
		scriptEl.async = true;
		scriptEl.src = src;
		var headEl = document.getElementsByTagName('head')[0];
		headEl.appendChild(scriptEl);
	}
	function logCheckAndInjectGAHeadCode(eventName) {
		var GA_ID = 'G-Z0TZ7TDHS1';
		if (!(window.gtag &amp;&amp; typeof window.gtag === 'function')) {
			var gaUrl = 'https://www.googletagmanager.com/gtag/js?id=' + GA_ID;
			window.dataLayer = window.dataLayer || [];
			window.gtag = function () {
				window.dataLayer.push(arguments);
			};
			injectHeadCodeOnPage(gaUrl);
		}
		var gaData = {
			send_to: GA_ID,
			value: 1,
			siteid: 37780
		};
		window.gtag('js', new Date());
		window.gtag('config', GA_ID, {
			send_page_view: false,
			custom_map: {
				dimension1: 'siteid'
			}
		});
		window.gtag('event', eventName, gaData);
	}

	if (window.location.pathname == '/') {
		return;
	}

	var pageHostname = window.location.hostname;
	var interLinks = document.querySelectorAll('#city a');

	for (var i = 0; i &lt; interLinks.length; i++) {
		var interLink = interLinks[i];
		var hostname = interLink.hostname;
		var hrefValue = interLink.getAttribute('href');
		var className = interLink.className;

		if (hrefValue &amp;&amp; hrefValue.includes('#')) {
			continue;
		}

		if (hostname != pageHostname) {
			continue;
		}

		if (
			interLink.rel &amp;&amp;
			(interLink.rel.includes('category') ||
				interLink.rel.includes('author') ||
				interLink.rel.includes('bookmark'))
		) {
			continue;
		}

		if (className &amp;&amp; (className.includes('next') || className.includes('prev'))) {
			continue;
		}

		if (interLink.innerText == 'Test it Now') {
			continue;
		}

		if (
			interLink &amp;&amp;
			interLink.parentElement &amp;&amp;
			interLink.parentElement.className &amp;&amp;
			interLink.parentElement.className.includes('nexttopiclink')
		) {
			continue;
		}

		interLink.onclick = function () {
			logCheckAndInjectGAHeadCode('interlink_clicked');
		};
	}
})();

(function () {
	var css = '.powered-by-banner { display: none !important; }',
		head = document.head || document.getElementsByTagName('head')[0],
		style = document.createElement('style');

	head.appendChild(style);

	style.type = 'text/css';
	if (style.styleSheet) {
		style.styleSheet.cssText = css;
	} else {
		style.appendChild(document.createTextNode(css));
	}
})();

/* 6sense Tag start */

(function () {
	window._6si = window._6si || [];
	window._6si.push(['enableEventTracking', true]);
	window._6si.push(['setToken', '0a183be4b8f4234161b8d67b351ea76a']);
	window._6si.push(['setEndpoint', '//b.6sc.co/']);
	(function () {
		var gd = document.createElement('script');
		gd.type = 'text/javascript';
		gd.async = true;
		gd.src = '//j.6sc.co/6si.min.js';
		var s = document.getElementsByTagName('script')[0];
		s.parentNode.insertBefore(gd, s);
	})();
})();

/* 6sense tag end */
(function () {
	var googletag = (window.googletag = window.googletag || {});
	googletag.cmd = googletag.cmd || [];
	googletag.cmd.push(function () {
		function checkPubAds() {
			if (!googletag.pubadsReady) {
				setTimeout(checkPubAds, 0);
			} else {
				googletag.pubads().addEventListener('slotRenderEnded', function (e) {
					try {
						var adUnitCode = e.slot.getSlotId().getDomId();
						var s = '#' + adUnitCode;
						var adUnitElement = window.adpushup.$(s);
						var ap_apex_ad = adUnitElement.parent();
						if (!e.isEmpty &amp;&amp; e.size[0] &gt; 1 &amp;&amp; e.size[1] &gt; 1) {
							ap_apex_ad.css('height', e.size[1]);
							ap_apex_ad.css('width', e.size[0]);
						} else if (e.isEmpty) {
							ap_apex_ad.css('height', '0px');
							ap_apex_ad.css('width', '0px');
						}
					} catch (e) {
						triggerRequest('JTP_slotRenderEndedError', {
							msg: JSON.parse(JSON.stringify(e, Object.getOwnPropertyNames(e))),
							url: window.location.href
						});
					}
				});
			}
		}
		checkPubAds();
	});
})();

// Link Previews Starts
(function () {
	var scriptTag = document.createElement('script');
	var linkPreviewUrl =
		'https://cdn.adpushup.com/' + window.adpushup.config.siteId + '/linkPreview.js';
	scriptTag.src = linkPreviewUrl;
	document.body.append(scriptTag);
})();
// Link Previews Ends

// Bidder Info logging starts
(function () {
	var adpushup = window.adpushup || {};
	adpushup.que = adpushup.que || [];
	adpushup.que.push(async function () {
		var apConfig = (adpushup &amp;&amp; adpushup.config) || {};
		var siteId = apConfig.siteId;
		var country = apConfig.country;
		var platform = apConfig.platform;
		var date = Date.now();
		var userId = await adpushup.utils.getAPUserId();

		function addMetaDataToObj(obj) {
			obj.siteId = siteId;
			obj.country = country;
			obj.platform = platform;
			obj.date = date;
			obj.url = window.location.pathname;
			obj.userId = userId;
			obj.sessionId = adpushup.utils.getAPSessionId();
			obj.auctionId = window.auctionId;
		}
		function sendDataToLoggerService(payload, event) {
			var eventName = event + '_' + siteId;
			var endPoint = 'https://aplogger.adpushup.com/log?pxRes=false&amp;detectGeo=true';
			var strigifiedData = JSON.stringify({ event: eventName, data: payload });
			if (strigifiedData === '{}') {
				strigifiedData = JSON.stringify(
					payload,
					Object.getOwnPropertyNames({ event: eventName, data: payload })
				);
			}
			window.navigator.sendBeacon(endPoint, strigifiedData);
		}

		var _apPbJs = window._apPbJs || {};
		_apPbJs.que = _apPbJs.que || [];
		_apPbJs.que.push(function () {
			_apPbJs.onEvent('auctionEnd', function (ev) {
				var uniqueBiddersList = [];
				var bidsReceived =
					ev.bidsReceived.map((obj) =&gt; {
						obj.didSendABid = true;
						return obj;
					}) || [];
				ev.noBids.forEach((bidData) =&gt; {
					if (!uniqueBiddersList.includes(bidData.bidder)) uniqueBiddersList.push(bidData.bidder);
				});
				if (!bidsReceived.length) return [];
				let bidsDataToSend = [];
				for (var i = 0; i &lt; bidsReceived.length; i++) {
					var bidDetails = {};
					var bidData = bidsReceived[i];
					if (!uniqueBiddersList.includes(bidData.bidderCode))
						uniqueBiddersList.push(bidData.bidderCode);
					if (i === 0) window.auctionId = bidData.auctionId;
					var cpm = bidData.didSendABid ? bidData.cpm : null;
					bidDetails['cpm'] = cpm;
					bidDetails['sectionId'] = bidData.adUnitCode;
					bidDetails['responseTime'] = bidData.timeToRespond;
					bidDetails['refreshCount'] = window.adpTags.adpSlots[bidData.adUnitCode].refreshCount;
					bidDetails['network'] = bidData.bidderCode;
					bidDetails['didSendABid'] = bidData.didSendABid;
					bidsDataToSend.push(bidDetails);
				}

				if (!bidsDataToSend.length) return;
				var auctionEndData = {};
				var uniqueBidderData = {
					bidderList: uniqueBiddersList,
					siteId: siteId,
					country: country,
					platform: platform
				};
				auctionEndData['auctionDetails'] = bidsDataToSend;
				addMetaDataToObj(auctionEndData);
				sendDataToLoggerService(auctionEndData, 'BidderLogging_auctionEnd');
				sendDataToLoggerService(uniqueBidderData, 'autoRuleVerificationLogs');
			});
			_apPbJs.onEvent('bidWon', function (ev) {
				var bidWonData = {};
				var auctionDetails = {};
				auctionDetails['sectionId'] = ev.adUnitCode;
				auctionDetails['refreshCount'] = window.adpTags.adpSlots[ev.adUnitCode].refreshCount;
				auctionDetails['network'] = ev.bidderCode;
				auctionDetails['cpm'] = ev.cpm;
				bidWonData['auctionDetails'] = auctionDetails;
				addMetaDataToObj(bidWonData);
				sendDataToLoggerService(bidWonData, 'BidderLogging_bidWon');
			});
		});
	});
})();
// Bidder Info logging ends

/*IL_JOB_STARTS*/
(function () {
	var LOG_ENDPOINT_1 = 'https://bi.adpushup.com/log?pxRes=false';

	//avoid message trigger twice for pnp accounts
	if (window.IL_MessageTriggered) return;
	var adpushup = window.adpushup || {};
	adpushup.que = adpushup.que || [];
	adpushup.que.push(function () {
		var w = window;
		var ap = adpushup;
		var EU_COUNTRY_LIST = [
			'AT',
			'BE',
			'BG',
			'HR',
			'CY',
			'CZ',
			'DK',
			'EE',
			'FI',
			'FR',
